pipeline {
    agent any

    tools {
        maven 'maven'
    
        
    }

    stages {

        stage('git clone') {
            steps {
            git branch: 'main', credentialsId: 'git', url: 'https://github.com/namastemama/ks.git'
                
            }    
        }

        stage('maven version') {
            steps {
            sh 'mvn --version'
                
            }    
        }
        stage('mvn clean') {
            steps {
            sh 'mvn clean'
                
            }    

        }
        stage('sonarqube scan'){
            steps {
            mvn sonar:sonar \
              -Dsonar.projectKey=ksproject \
              -Dsonar.host.url=http://34.125.76.60:9000 \
              -Dsonar.login=ad287cca249518e3c175e07a2cf8e612f7771b50
            }

        }



        stage('validate') {
            steps {
            sh 'mvn validate'
                
            }    
        }
        stage('compile') {
            steps {
            sh 'mvn compile'
                
            }    
        }
        stage('test') {
            steps {
            sh 'mvn test'
                
            }    
        }
        stage('package') {
            steps {
            sh 'mvn package'
                
            }    
        }
        stage('deploy') {
            steps {
            sh 'mvn deploy'
                
            }    
        }




    }
}
