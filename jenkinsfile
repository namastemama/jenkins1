node {
    
    stage('git clone') { 
        git branch: 'main', credentialsId: 'git', url: 'https://github.com/namastemama/ks.git'
    }
    stage('maven clean') { 
        sh 'mvn clean'
    }
    stage('validate') {
        sh 'mvn validate'
        
    }
    stage('compile') {
        sh 'mvn compile'
        
    }
    stage('test') {
        sh 'mvn test'
        
    }
    stage('package') {
        sh 'mvn package'
        
    }
    stage('deployment') {
        sh 'mvn deploy'
        
    }
}
